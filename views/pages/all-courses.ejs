                <div class="col-lg-12 sw-as-center mb-30">
  
                  <!-- title -->
                  <h4 class="sw-h sw-h-lvl-4 mb-30">Agile Project Management</h4>
                  <!-- suptitle -->

                </div>
  
              </div>
  
            </div>
  
            <div class="col-lg-12">
  
              <div class="row">
  
                <% 
                
                    let agileCourses = courses.filter(c => c.type == "agile")
                    for(let i = 0; i< agileCourses.length; i++){
                    let course = agileCourses[i]
                    %>
                    <!-- Course start-->
                    <div class="col-lg-4">
    
                    <!-- card -->
                    <div class="sw-card">
                        <!-- img frame -->
                        <a href="/courses/<%= course.moniker %>" class="sw-img">
                        <img src="<%= course.courseImage == "" || course.courseImage == null ? "/public/img/business/services/1.png" : course.courseImage %>" alt="thumb">
                        </a>
                        <!-- img frame end -->
                        <!-- description -->
                        <div class="sw-card-description">
                        <!-- title -->
                        <a href="/courses/<%= course.moniker %>" class="mb-15">
                            <h5><%= course.title %></h5>
                        </a>
                        <!-- short description -->
                        <div> 
                            <%= course.introduction.body.replace( /(<([^>]+)>)/ig, '').substring(0,100) %>...
                            </div>
                        <!-- button -->
                        <!-- button -->
                        </div>
                        <!-- description end -->
                        <!-- badge -->
                    </div>
                    <!-- card end -->
    
                    </div>
                    <!-- Course end -->
                    
                    <%
