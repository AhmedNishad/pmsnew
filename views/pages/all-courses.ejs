                <svg width="100%">
                  <text x="0" y="140px" font-size="150px" stroke="#3e3e41" stroke-width="4" fill="transparent">
                    <!-- section title -->
                    Courses
                  </text>
                </svg>
              </div>
  
            </div>
  
            <!--Agile Start-->

            <div class="col-lg-12">
  
