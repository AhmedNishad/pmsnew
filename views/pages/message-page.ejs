        <!-- overlay -->
        <div class="sw-overlay sw-overlay-left-1">
          <div class="container sw-d-flex">
            <!-- main title frame -->
            <div class="sw-main-title sw-main-title-left sw-animation-opas sw-top-header-fix">
              <!-- title -->
              <h2 class="sw-white">Register Now!</h1>
              
